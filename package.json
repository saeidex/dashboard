{
  "name": "takumitex",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run -r build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "pnpm run -r clean",
    "db:push": "pnpm -F @takumitex/api db:push",
    "db:generate": "pnpm -F @takumitex/api db:generate",
    "db:studio": "pnpm -F @takumitex/api db:studio",
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "dev:web": "pnpm -F @takumitex/web dev",
    "dev:api": "pnpm -F @takumitex/api dev",
    "dev:api-client": "pnpm -F @takumitex/api-client dev",
    "docker:build": "docker compose build",
    "docker:build:api": "docker compose build api",
    "docker:build:web": "docker compose build web",
    "docker:db:studio": "docker compose exec api db:studio",
    "docker:down": "docker compose down",
    "docker:logs:api": "docker compose logs -f api",
    "docker:logs:web": "docker compose logs -f web",
    "docker:ps": "docker compose ps",
    "docker:up": "docker compose up -d",
    "lint": "pnpm run -r --parallel lint",
    "lint:fix": "pnpm run -r --parallel lint:fix",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm run lint:ws",
    "start:api": "pnpm -F @takumitex/api start",
    "start:web": "pnpm -F @takumitex/web preview --host",
    "typecheck": "pnpm run -r --parallel typecheck",
    "ui-add": "pnpm -F @takumitex/web ui-add"
  },
  "devDependencies": {
    "@takumitex/eslint-config": "workspace:^",
    "eslint": "^9.36.0",
    "typescript": "^5.9.2"
  },
    "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.19"
    }
  }
}
